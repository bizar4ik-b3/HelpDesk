        @Html.DisplayFor(model => model.ShortDepartmentName)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.PersonCount)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.PersonCount)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.GroupCourse)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.GroupCourse)
