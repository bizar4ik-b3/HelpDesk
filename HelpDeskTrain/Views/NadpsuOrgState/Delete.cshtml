@model HelpDeskTrain.Models.NadpsuOrgState

@{
    ViewBag.Title = "Видалення запису";
}

<h2>Видалення запису</h2>

<h3>Ви впевнені що бажаете видалити запис?</h3>
<fieldset>
    <legend>Орг штат НАДПСУ</legend>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.GroupNumber)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.GroupNumber)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.DepartmentName)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.DepartmentName)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.ShortDepartmentName)
    </div>
